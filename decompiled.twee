body, tw-story
{
  font-family: Helvetica;
  font-size: 17px;
}

.twocolumn {
	-webkit-column-count: 2;
	-moz-column-count: 2;
	column-count: 2;
}

.leftcolumn {
	float: left;
  	width: 40%;
  	margin-right: 20%;
}

.otherleftcolumn {text-align: left;
    margin-left: 15px;
float: left;
  	width: 40%;
  	margin-right: 20%;}

.otherrightcolumn {text-align: right;
    margin-right: 15px;
float: right;
  	width: 40%;
  	margin-left: 20%;}

.rightcolumn {
  margin-right: 20%;
  margin-left: 60%;
}

.center {
	text-align: center;
}

.right {text-align: right;}

.rightimg {
  right: 0px;
}

.Phone a {color: #F6CEEC;
	text-shadow: 0em 0em 0.08em #FFF;}



.ship {background: #000000;
	color: #C0C0C0;	
	border-style: double;
	border-width: 20px;
	border-color: #3E3E3E;
	margin-left: 10px;
	padding-top: 20px;
	padding-bottom: 20px;
	padding-left: 40px;
	padding-right: 20px;
	height: 400px;
	width: 620px;}

.sounds {
	font-size: 22px; text-align: center;
}

.shadow {
	text-shadow: 0.08em 0.08em 0.08em #FFF;
}
.emboss {
	text-shadow: 0.08em 0.08em 0em #FFF;
	color: black;
}
.blur {
	color: transparent;
	text-shadow: 0em 0em 0.08em #FFF;
}
.blurrier {
	color: transparent;
	text-shadow: 0em 0em 0.2em #FFF;
}
.blurrier::selection {
	background-color: transparent;
	color: transparent;
}
.blurrier::-moz-selection {
	background-color: transparent;
	color: transparent;
}
.smear {
	color: transparent;
	text-shadow:
		0em    0em 0.02em rgba(255,255,255,0.75),
		-0.2em 0em 0.5em  rgba(255,255,255,0.5),
		0.2em  0em 0.5em  rgba(255,255,255,0.5);
}
.mirror {
	display: inline-block;
	transform: scaleX(-1);
	-webkit-transform: scaleX(-1);
}
.upside-down {
	display: inline-block;
	transform: scaleY(-1);
	-webkit-transform: scaleY(-1);
}

@-webkit-keyframes fade-ins-out {
	0%,
	to {opacity: 0}
	50% {opacity: 1}
}
@keyframes fade-ins-out {
	0%,
	to {opacity: 0}
	50% {opacity: 1}
}
.fade-ins-out {
	text-decoration: none;
	animation: fade-in-out .8s ease-in-out infinite alternate;
	-webkit-animation: fade-in-out .8s ease-in-out infinite alternate;
}

@-webkit-keyframes fade-in-out {
	0%,
	to {opacity: 0}
	50% {opacity: 1}
}
@keyframes fade-in-out {
	0%,
	to {opacity: 0}
	50% {opacity: 1}
}
.fade-in-out {
	text-decoration: none;
	animation: fade-in-out 2s ease-in-out infinite alternate;
	-webkit-animation: fade-in-out 2s ease-in-out infinite alternate;
}

@-webkit-keyframes rumble {
	50% {
		-webkit-transform: translateY(-.2em);
		transform: translateY(-.2em)
	}
}
@keyframes rumble {
	50% {
		-webkit-transform: translateY(-.2em);
		transform: translateY(-.2em)
	}
}
.rumble {
	-webkit-animation: rumble linear 0.1s 0s infinite;
	animation: rumble linear 0.1s 0s infinite;
	display:inline-block;
}

.rumbleshort {
	-webkit-animation: rumble linear 0.1s 0s infinite;
	animation: rumble linear 0.1s 0s infinite;
	display:inline-block;
    animation-iteration-count: 10;
}



@-webkit-keyframes shudder {
	50% {
		-webkit-transform: translateX(0.2em);
		transform: translateX(0.2em)
	}
}
