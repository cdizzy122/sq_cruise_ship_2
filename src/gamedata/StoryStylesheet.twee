body, tw-story
{
  font-family: Helvetica;
  font-size: 17px;
}

.twocolumn {
	-webkit-column-count: 2;
	-moz-column-count: 2;
	column-count: 2;
}

.leftcolumn {
	float: left;
  	width: 40%;
  	margin-right: 20%;
}

.otherleftcolumn {text-align: left;
    margin-left: 15px;
float: left;
  	width: 40%;
  	margin-right: 20%;}

.otherrightcolumn {text-align: right;
    margin-right: 15px;
float: right;
  	width: 40%;
  	margin-left: 20%;}

.rightcolumn {
  margin-right: 20%;
  margin-left: 60%;
}

.center {
	text-align: center;
}

.right {text-align: right;}

.rightimg {
  right: 0px;
}

.Phone a {color: #F6CEEC;
	text-shadow: 0em 0em 0.08em #FFF;}



.ship {background: #000000;
	color: #C0C0C0;	
	border-style: double;
	border-width: 20px;
	border-color: #3E3E3E;
	margin-left: 10px;
	padding-top: 20px;
	padding-bottom: 20px;
	padding-left: 40px;
	padding-right: 20px;
	height: 400px;
	width: 620px;}

.sounds {
	font-size: 22px; text-align: center;
}

.shadow {
	text-shadow: 0.08em 0.08em 0.08em #FFF;
}
.emboss {
	text-shadow: 0.08em 0.08em 0em #FFF;
	color: black;
}
.blur {
	color: transparent;
	text-shadow: 0em 0em 0.08em #FFF;
}
.blurrier {
	color: transparent;
	text-shadow: 0em 0em 0.2em #FFF;
}
.blurrier::selection {
	background-color: transparent;
	color: transparent;
}
.blurrier::-moz-selection {
	background-color: transparent;
	color: transparent;
}
.smear {
	color: transparent;
	text-shadow:
		0em    0em 0.02em rgba(255,255,255,0.75),
		-0.2em 0em 0.5em  rgba(255,255,255,0.5),
		0.2em  0em 0.5em  rgba(255,255,255,0.5);
}
.mirror {
	display: inline-block;
	transform: scaleX(-1);
	-webkit-transform: scaleX(-1);
}
.upside-down {
	display: inline-block;
	transform: scaleY(-1);
	-webkit-transform: scaleY(-1);
}

@-webkit-keyframes fade-ins-out {
	0%,
	to {opacity: 0}
	50% {opacity: 1}
}
@keyframes fade-ins-out {
	0%,
	to {opacity: 0}
	50% {opacity: 1}
}
.fade-ins-out {
	text-decoration: none;
	animation: fade-in-out .8s ease-in-out infinite alternate;
	-webkit-animation: fade-in-out .8s ease-in-out infinite alternate;
}

@-webkit-keyframes fade-in-out {
	0%,
	to {opacity: 0}
	50% {opacity: 1}
}
@keyframes fade-in-out {
	0%,
	to {opacity: 0}
	50% {opacity: 1}
}
.fade-in-out {
	text-decoration: none;
	animation: fade-in-out 2s ease-in-out infinite alternate;
	-webkit-animation: fade-in-out 2s ease-in-out infinite alternate;
}

@-webkit-keyframes rumble {
	50% {
		-webkit-transform: translateY(-.2em);
		transform: translateY(-.2em)
	}
}
@keyframes rumble {
	50% {
		-webkit-transform: translateY(-.2em);
		transform: translateY(-.2em)
	}
}
.rumble {
	-webkit-animation: rumble linear 0.1s 0s infinite;
	animation: rumble linear 0.1s 0s infinite;
	display:inline-block;
}

.rumbleshort {
	-webkit-animation: rumble linear 0.1s 0s infinite;
	animation: rumble linear 0.1s 0s infinite;
	display:inline-block;
    animation-iteration-count: 10;
}



@-webkit-keyframes shudder {
	50% {
		-webkit-transform: translateX(0.2em);
		transform: translateX(0.2em)
	}
}
@keyframes shudder {
	50% {
		-webkit-transform: translateX(0.2em);
		transform: translateX(0.2em)
	}
}
.shudder {
	-webkit-animation: shudder linear 0.1s 0s infinite;
	animation: shudder linear 0.1s 0s infinite;
	display: inline-block;
}

@-webkit-keyframes fade-in {
	0%,
	to {opacity: 0}
	100% {opacity: 1}
}
@keyframes fade-in {
	0%,
	to {opacity: 0}
	100% {opacity: 1}
}
.fade-in {
	text-decoration: none;
	animation: fade-in 4s ease-in;
	-webkit-animation: fade-in 4s ease-in;
}
@-webkit-keyframes fade-out {
	0%,
	to {opacity: 1}
    100% {opacity: 0}
}
@keyframes fade-out {
	0%,
	to {opacity: 1}
	100% {opacity: 0}
}
.fade-out {
	text-decoration: none;
	animation: fade-out 4s ease-out;
	-webkit-animation: fade-out 4s ease-out forwards;
}

@-webkit-keyframes pulse {
	0%  { -webkit-transform: scale(0, 0); transform: scale(0, 0) }
	20% { -webkit-transform: scale(1.2, 1.2); transform: scale(1.2, 1.2) }
	40% { -webkit-transform: scale(0.9, 0.9); transform: scale(0.9, 0.9) }
	60% { -webkit-transform: scale(1.05, 1.05); transform: scale(1.05, 1.05) }
	80% { -webkit-transform: scale(0.925, 0.925); transform:scale(0.925, 0.925) }
	to  { -webkit-transform: scale(1, 1); transform: scale(1, 1) }
}
@keyframes pulse {
	0%  { -webkit-transform: scale(0, 0); transform: scale(0, 0) }
	20% { -webkit-transform: scale(1.2, 1.2); transform: scale(1.2, 1.2) }
	40% { -webkit-transform: scale(0.9, 0.9); transform: scale(0.9, 0.9) }
	60% { -webkit-transform: scale(1.05, 1.05); transform: scale(1.05, 1.05) }
	80% { -webkit-transform: scale(0.925, 0.925); transform: scale(0.925, 0.925) }
	to  { -webkit-transform: scale(1, 1); transform: scale(1, 1) }
}
.pulse {
	-webkit-animation: pulse 3s;
	animation: pulse 3s;
	display: inline-block;
}
@-webkit-keyframes fade-ins {
	0%,
	to {opacity: 0}
	100% {opacity: 1}
}
@keyframes fade-ins {
	0%,
	to {opacity: 0}
	100% {opacity: 1}
}
.fade-ins {
	text-decoration: none;
	animation: fade-ins 2s ease-in;
  -webkit-animation: fade-ins 2s ease-in;}

@-webkit-keyframes fade-inss {
	0%,
	to {opacity: 0}
	100% {opacity: 1}
}
@keyframes fade-inss {
	0%,
	to {opacity: 0}
	100% {opacity: 1}
}
.fade-inss {
	text-decoration: none;
	animation: fade-inss 6s ease-in;
  -webkit-animation: fade-inss 6s ease-in;}





/* KEYFRAMES */

@-webkit-keyframes animateBubble {
    0% {
        margin-top: 1000px;
    }
    100% {
        margin-top: -100%;
    }
}

@-moz-keyframes animateBubble {
    0% {
        margin-top: 1000px;
    }
    100% {
        margin-top: -100%;
    }
}

@keyframes animateBubble {
    0% {
        margin-top: 1000px;
    }
    100% {
        margin-top: -100%;
    }
}

@-webkit-keyframes sideWays { 
    0% { 
        margin-left:0px;
    }
    100% { 
        margin-left:50px;
    }
}

@-moz-keyframes sideWays { 
    0% { 
        margin-left:0px;
    }
    100% { 
        margin-left:50px;
    }
}

@keyframes sideWays { 
    0% { 
        margin-left:0px;
    }
    100% { 
        margin-left:50px;
    }
}

/* ANIMATIONS */

.x1 {
    -webkit-animation: animateBubble 10s linear infinite, sideWays 2s ease-in-out infinite alternate;
	-moz-animation: animateBubble 10s linear infinite, sideWays 2s ease-in-out infinite alternate;
	animation: animateBubble 10s linear infinite, sideWays 2s ease-in-out infinite alternate;
	
